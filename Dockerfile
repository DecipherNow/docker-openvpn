FROM alpine:3.8

LABEL maintainer="Joshua Rutherford <joshua.rutherford@deciphernow.com>"

ENV CONTAINER_NETMASK ""
ENV CONTAINER_NETWORK ""
ENV DH_PARAM ""
ENV SERVICE_NETMASK ""
ENV SERVICE_NETWORK ""
ENV TLS_CA_CERTIFICATE ""
ENV TLS_SERVER_CERTIFICATE ""
ENV TLS_SERVER_KEY ""
ENV VERBOSITY "4"
ENV VPN_NETMASK ""
ENV VPN_NETWORK ""

RUN apk add --no-cache \
  openvpn

COPY files/ /

RUN mkdir -p /var/lib/openvpn

EXPOSE 1194
VOLUME ["/etc/openvpn/tls"]

ENTRYPOINT ["/usr/local/bin/entrypoint"]

CMD ["/usr/sbin/openvpn", "--config", "/etc/openvpn/openvpn.conf"]
